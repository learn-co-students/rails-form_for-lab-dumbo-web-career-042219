<form action="/students/<%= @student.id %>" method="POST">
  <input type="hidden" name="_method" value="PATCH">
  First Name<br>
  <input type="text" name="student_first_name" value=<%= @student.first_name%>><br>

  Last Name<br>
  <input type="text" name="student_last_name" value=<%= @student.last_name%>><br>

  <input type="submit" value="Update Student"> 
</form>